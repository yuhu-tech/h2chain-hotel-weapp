/* pages/h2-order/prompt-success/prompt-success.wxss */

/* background image */

.page_bg {
  position: fixed;
  width: 100%;
  height: 100%;
}

/* center */

.page_text {
  position: fixed;
  top: 430rpx;
  width: 100%;
}

.page_text_box {
  display: flex;
  justify-content: center;
  font-size: 36rpx;
}

/* bottom */

.page_btm {
  position: fixed;
  bottom: 0;
  width: 100%;
}

.page_btm_box {
  padding: 0 40rpx 50rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
}

.page_btm_box button {
  margin: 50rpx 0 0;
  width: 500rpx;
}
